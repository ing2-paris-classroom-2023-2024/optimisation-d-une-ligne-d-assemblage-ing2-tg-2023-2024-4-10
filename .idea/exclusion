#include <stdio.h>
#include <stdlib.h>

#define MAX_STATIONS 100

int main() {
    FILE *file = fopen("exclusion.txt", "r");
    if (file == NULL) {
        fprintf(stderr, "Erreur lors de l'ouverture du fichier exclusion.txt\n");
        return 1;
    }
    int n, i, j;
    fscanf(file, "%d", &n);

    int V[n];
    int E[n][2];

    for (int k = 0; k < n; k++) {
        fscanf(file, "%d %d", &i, &j);
        E[k][0] = i;
        E[k][1] = j;
    }
